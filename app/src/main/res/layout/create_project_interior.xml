<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffffff">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="#00ffffff"
        android:paddingBottom="16dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="16dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="Create a Project"
            android:textAppearance="?android:attr/textAppearanceLarge" />

    </RelativeLayout>

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/interiorScroll">

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="16dp"
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:paddingTop="16dp"
            android:id="@+id/createProjectIntLayout">

            <TextView
                android:id="@+id/topOfInterior"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:paddingBottom="16dp"
                android:text="What does the project consist of?"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <TextView
                android:id="@+id/wallsInt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="walls"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Repair"
                android:theme="@style/switchStyle"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Painting"
                android:theme="@style/switchStyle"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="16dp"
                android:text="flooring repair"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Chipped Tile"
                android:theme="@style/switchStyle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Damaged Wood"
                android:theme="@style/switchStyle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Carpet"
                android:theme="@style/switchStyle"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="16dp"
                android:text="flooring installation"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Tile"
                android:theme="@style/switchStyle"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Wood"
                android:theme="@style/switchStyle"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Carpet"
                android:theme="@style/switchStyle"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="16dp"
                android:text="plumbing"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Toilet Clog"
                android:theme="@style/switchStyle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Troubleshooting a Leak"
                android:theme="@style/switchStyle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="16dp"
                android:text="electrical"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
