                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="General"
                android:theme="@style/switchStyle"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="16dp"
                android:text="cooling and heating"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Troubleshooting"
                android:theme="@style/switchStyle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="16dp"
                android:text="cleaning"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Home Touch-Up"
                android:theme="@style/switchStyle"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Home Deep-Clean"
                android:theme="@style/switchStyle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="16dp"
                android:text="other"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Fixture Installation"
                android:theme="@style/switchStyle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Troubleshooting Appliances"
                android:theme="@style/switchStyle"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/darker_gray" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="16dp"
                android:text="carpet"
                android:textAppearance="?android:attr/textAppearanceSmall" />

            <android.support.v7.widget.SwitchCompat
                android:id="@+id/switchip17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:checked="false"
                android:paddingBottom="20dp"
                android:paddingTop="20dp"
                android:text="Cleaning"
                android:theme="@style/switchStyle" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginBottom="25dp"
                android:background="@android:color/darker_gray" />

            <android.support.v7.widget.AppCompatButton
                android:id="@+id/interiorProject"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:text="Next"
                android:onClick="onProgressiveClick"
                android:textAllCaps="false"
                android:backgroundTint="#B6AFA9"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
